                },
                {
                    "bit": 4,
                    "data_type": "AMPS",
                    "key": "CHARGE_AMPS",
                    "title": "Charge Current"
                },
                {
                    "bit": 5,
                    "data_type": "VOLTS",
                    "key": "SOLAR_VOLTS",
